<Window x:Class="AWSS3_UEM.Ventanas.VisorResultados"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AWSS3_UEM.Ventanas"
        mc:Ignorable="d"
        Title="VisorResultados" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dtGridResultados" Margin="0,0,0,62" IsReadOnly="True" MouseDoubleClick="dtGridResultados_MouseDoubleClickAsync" />
        <Button x:Name="btnCerrar" Content="Cerrar" Margin="0,0,0,7" Width="120" Click="btnCerrar_Click" HorizontalAlignment="Center" Height="28" VerticalAlignment="Bottom" />
        <Label x:Name="lblinfoDblClick" Content="Doble click para abrir la imagen" HorizontalAlignment="Center" Width="188" Height="23" VerticalAlignment="Bottom" Margin="0,0,0,39"/>

    </Grid>
</Window>
